# Docker Compose Development Environment
# Copy this file to .env to override any defaults
# All variables are OPTIONAL - docker-compose.yml has sensible defaults for development

# ========================================
# All variables below have defaults in docker-compose.yml
# Only create a .env file if you need to override them
# ========================================

# Master encryption key for sensitive data
# Default: dev-encryption-key-change-in-production
# ENC_MASTER_KEY=your_encryption_key_here

# Database password
# Default: postgres
# DB_PASSWORD=postgres

# JWT Secret for token signing
# Default: dev-secret-min-32-characters-long-change-in-production
# JWT_SECRET=dev-secret-min-32-characters-long-change-in-production

# WebAuthn Relying Party ID
# Default: localhost
# WEBAUTHN_RP_ID=localhost

# WebAuthn Relying Party Origin (where users access the app)
# Default: http://localhost:5173
# WEBAUTHN_RP_ORIGIN=http://localhost:5173

# Database name
# Default: moneyy
# DB_NAME=moneyy

# Database user
# Default: postgres
# DB_USER=postgres
